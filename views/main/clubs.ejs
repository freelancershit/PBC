<% layout("layout") -%>
  <header id="myCarousel" class="carousel slide">
    <!-- Indicators -->
    <ol class="carousel-indicators">
      <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
      <li data-target="#myCarousel" data-slide-to="1"></li>
      <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner">
      <div class="item active">
        <div class="fill" style="background-image:url('/images/h1.jpg');"></div>
        <div class="carousel-caption">
        </div>
      </div>
      <div class="item">
        <div class="fill" style="background-image:url('/images/h2.jpg');"></div>
        <div class="carousel-caption">
        </div>
      </div>
      <div class="item">
        <div class="fill" style="background-image:url('/images/h3.jpg');"></div>
        <div class="carousel-caption">
        </div>
      </div>
    </div>

    <!-- Controls -->
    <a class="left carousel-control" href="#myCarousel" data-slide="prev">
      <span class="icon-prev"></span>
    </a>
    <a class="right carousel-control" href="#myCarousel" data-slide="next">
      <span class="icon-next"></span>
    </a>
  </header>

  <div class="container">
    <div class="row">
      <div class="col-lg-12">
        <h1 class="page-header">
          SCHOOL ORGANIZATIONS AND CLUBS
        </h1>
        <br>
      </div>
    </div>
  </div>
  <div class="container">
    <div class="col-lg-12">
      <div class="panel panel-info">
        <div class="panel-heading" style="text-align:center;">
          <h4>
            <a> A. Academic Clubs </a>
          </h4>
        </div>
        <div class="panel-body">
          <ul>
            <li>AP Club</li>
            <li>Filipino Club</li>
            <li>Science Club</li>
            <li>English Club</li>
            <li>Math Club</li>
            <li>EPP Club</li>
          </ul>

        </div>

      </div>
    </div>
    <div class="col-lg-12">
      <div class="panel panel-info">
        <div class="panel-heading" style="text-align:center;">
          <h4>
            <a> B. Interest Clubs </a>
          </h4>
        </div>
        <div class="panel-body">
          <ul>
            <li>Glee Club</li>
            <li>Dance Club</li>
            <li>Photographers Club</li>
            <li>Book Lovers Club</li>
            <li>Sunshine Club</li>
            <li>Sports Club</li>
            <li>Editorial Board</li>
          </ul>
        </div>
      </div>
    </div>
  </div>